    {
        "name": "Ronan Guilloux",
        "email": "ronan.guilloux@gmail.com",
        "homepage": "http://github.com/ronanguilloux",
        "role": "Developer"
    }],
    "support": {
        "email": "ronan.guilloux@gmail.com"
    },
    "license": "GNU General Public License v3.0",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {"IsoCodes": "src/"}
    },
